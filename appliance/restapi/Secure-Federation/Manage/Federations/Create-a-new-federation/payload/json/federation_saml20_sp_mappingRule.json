{
    "configuration": {
        "assertionConsumerService": [
            {
                "binding": "post",
                "default": "false",
                "index": "1"
            },
            {
                "binding": "redirect",
                "default": "false",
                "index": "2"
            }
        ],
        "companyName": "ISVA L2 Application Dashboard",
        "ecpEnabled": "false",
