{
    "configuration": {
        "assertionSettings": {
            "assertionValidAfter": 300,
            "assertionValidBefore": 300
        },
        "companyName": "ISVA L2 DOCKER IDP",
        "ecpEnabled": "false",
        "encryptionSettings": {
            "decryptionKeyIdentifier": {
                "keystore": ""
            }
        },
        "extensionMapping": {
            "activeDelegateId": "skip-extension-map"
        },
        "identityMapping": {
            "activeDelegateId": "default-map",
            "properties": {
                "identityMappingRuleReference": "103"
            }
        },
        "messageIssuerFormat": "",
        "messageIssuerNameQualifier": "",
