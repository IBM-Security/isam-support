            },
            "signingOptions": {
                "signAuthnResponse": "true"
            },
            "validationOptions": {
                "validateAuthnRequest": "true"
            }
        },
        "singleSignOnService": [
            {
                "binding": "post"
            },
            {
                "binding": "redirect"
            }
        ]
